<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Add a new course</h1>
  <%= simple_form_for @course, html: { multipart: true }, url: instructor_courses_path do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :cost %>
  <br/>
  <p> Image Upload: </p>
  <%= f.file_field :image %>
  <br />
  <%= f.submit "Create", class: 'enjoy-css pull-right' %>
  <br />
<% end %>
<br />
</div>
